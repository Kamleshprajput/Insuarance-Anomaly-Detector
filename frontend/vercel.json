{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "rootDirectory": "frontend",
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://YOUR_RENDER_URL.onrender.com/:path*"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [{ "key": "x-vercel-enable-rewrite-caching", "value": "1" }]
      }
    ]
  }
  